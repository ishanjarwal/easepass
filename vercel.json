{
    "version": 2,
    "name": "easepass",
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "style.css",
            "use": "@vercel/static"
        },
        {
            "src": "script.js",
            "use": "@vercel/static"
        },
        {
            "src": "/public",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/public",
            "dest": "/public"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}